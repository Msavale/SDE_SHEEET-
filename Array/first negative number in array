#include <iostream>
//#include <bits/stdc++>
using namespace std;


int main(void){
    int n;
    cout<<"entre the size of array";
    cin>>n;
    int arr[n];
    cout<<"entre the element of array";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    //vector <int> v;
    int flag=0;
    for(int i=0;i<n;i++){
        for(int j=i;j<3;j++){
            if(arr[j]<0) {
                cout<<arr[j];
                flag++;}
            if(flag!=0) break;    
        }
    }
     return 0;
    
}
